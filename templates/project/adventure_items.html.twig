{# templates/adventure_items.html.twig #}
<div class="adventures">
	<ul>
		<h1>Clues</h1>
		{% for adventure in adventures %}
			<li>
				<strong></strong>
				{{ adventure.notes }}<br>
				<strong></strong>
				{{ adventure.codes }}<br>
				<strong></strong>
				{{ adventure.keys }}
			</li>
		{% else %}
			<li>Nothing here</li>
		{% endfor %}
	</ul>
</div>
